package de.thb.ws1617.mdsd.calendar.controllers

/*
 * Implement the controller here
 */
main {
    appName "THB Kalender"
    appVersion "0.1"
    modelVersion "0.1"
    startView InsertEventView
    onInitialized init
}

contentProvider Termin localTestStore {
    providerType local
}

action CustomAction init {
    bind action DataAction(load localTestStore) on InsertEventView.loadBtn.onTouch
    bind action DataAction(save localTestStore) on InsertEventView.saveBtn.onTouch
}